{
  "name": "web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "bun run prepare && tamagui build --target web ./src -- next build",
    "start": "next start",
    "lint": "eslint --max-warnings 0",
    "check-types": "bun run prepare && next typegen && tsc --noEmit",
    "prepare": "bun run orval:generate",
    "orval:generate": "orval --config orval.config.ts",
    "orval:watch": "orval --config orval.config.ts --watch"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.14.1",
    "@repo/ui": "*",
    "@tamagui/next-theme": "catalog:",
    "@tamagui/react-native-svg": "catalog:",
    "@tanstack/react-query": "^5.90.21",
    "next": "16.1.0",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-native-web": "^0.21.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@tamagui/cli": "catalog:",
    "@tamagui/core": "catalog:",
    "@types/node": "^22.15.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "^9.39.1",
    "orval": "^8.3.0",
    "typescript": "5.9.2"
  }
}
